<!doctype html>
<!--[if lt IE 7 ]><html class="ie ie6" lang="en"> <![endif]-->
<!--[if IE 7 ]><html class="ie ie7" lang="en"> <![endif]-->
<!--[if IE 8 ]><html class="ie ie8" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie ie9" lang="en"> <![endif]-->
<!--[if (gte IE 10)|!(IE)]><!--><html lang="en"> <!--<![endif]-->
  <head>
    <meta charset="utf-8" />
     <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    {+metaTags/}
    <title>{+pageTitle}Links.to: Page{/pageTitle}</title>
    <script>pageParam={pageParam|js|s};</script>
    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/headjs/0.99/head.min.js"></script>
    <script>
      head.js(
          {socket_io_core:"/socket.io/socket.io.js"},
          {socket_main:"/js/links-to/socket-io.js"}, function(){
             console.log( 'linksTo socket-io has loaded');
          }
      );
      head.load(
          "//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.no-icons.min.css"
          , "//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css"
          , "/css/links-to.css"
          , "/css/hide.css"
      );
      var pageScripts=[], pageCSS=[];
    </script>
    {+headLinks/}
    {+headContent/}
  </head>
  <body class="{+bodyClass/}">
    <div id="navbar">{>"navigation/navbar"/}</div>
    <div class="container" id="container">
        {+container}
          <div id="breadcrumbs">{>"navigation/breadcrumbs"/}</div>
          <div class="row"><div class="span8" id="title">{+title}<h1>{title}</h1>{/title}</div></div>
          <div id="beforeContent">{+beforeContent/}</div>
          <div id="content">{+content}_content_{/content}</div>
          {+contentBottom/}
       {/container}
    </div>
    {>mainPageScripts/}
    {+pageScripts/}
    <script>
        if( pageScripts.length) { head.js.applay(undefined,   pageScripts ); }
        if( pageCSS.length)     { head.load.applay(undefined, pageCSS ); }
    </script>

    {+pageBottom /}
  </body>
</html>