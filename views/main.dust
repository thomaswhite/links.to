<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    {+metaTags}
      <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    {/metaTags}
    <title{+pageTitle}>Links.to: Page{/pageTitle%}</title>
    {+headLInks}
    <link href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.1/css/bootstrap-combined.no-icons.min.css" rel="stylesheet"/>
    <link href="//netdna.bootstrapcdn.com/font-awesome/3.1.1/css/font-awesome.css" rel="stylesheet"/>
    <link href="/css/links-to.css" rel="stylesheet" />
    {/headLInks}

    {+headContent}{/headContent}
    <script>pageData = { dummy:0 {+pageData}{/pageData} }</script>
  </head>
  <body calss="{bodyClass}">
    {+navbar}navbar{/navbar>
    <div class="container">
      {+breadcrumbs}breadcrumbs{/breadcrumbs}
      <div class="row"><div class="span8 error">{% block error %}{% endblock %}</div></div>
      <div class="row"><div class="span8">{+title}<h1>{{ title }}</h1>{/title}</div></div>
      {+content} _content_ {/content}
      {+contentRows/}
      {+contentBottom/}
    </div>
    <script>
        function fallback(fn, src ){
            if ( !fn ) {
                document.write(unescape("%3Cscript src='" + src + "' type='text/javascript'%3E%3C/script%3E"));
            }
        }
    </script>
    <!-- script type="text/javascript" src="/bootstrap/js/bootstrap.min.js"></script -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script type="text/javascript">fallback( window.jQuery, '/js/jquery-1.8.3.min.js'  );</script>

    <script src="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.1/js/bootstrap.min.js"></script>
    <script type="text/javascript">fallback( $ && $.tn ? $.fn.affix : null, '/js/jquery-1.8.3.min.js'  );</script>

    <script type="text/javascript">
        // TODO find some other way to check for CDN CSS fallback
        if ( 0 &&  typeof jQuery == 'undefined') {
            document.write(unescape('%3Clink rel="stylesheet" type="text/css" href="/bootstrapcdn-offline/2.3.1/bootstrap-combined.no-icons.min.css" /%3E'));
            document.write(unescape('%3Clink rel="stylesheet" type="text/css" href="/font-awesome/css/font-awesome.min.css" /%3E'));
            document.write(unescape('%3Clink rel="stylesheet" type="text/css" href="/css/links-to.css" /%3E'));
        }
    </script>
    <script type="text/javascript" src="/socket.io/socket.io.js"></script>
    <script type="text/javascript" src="/js/socket-io_client.js"></script>
    {+pageBottom /}
  </body>
</html>