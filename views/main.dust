<!doctype html>
<!--[if lt IE 7 ]><html class="ie ie6" lang="en"> <![endif]-->
<!--[if IE 7 ]><html class="ie ie7" lang="en"> <![endif]-->
<!--[if IE 8 ]><html class="ie ie8" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie ie9" lang="en"> <![endif]-->
<!--[if (gte IE 10)|!(IE)]><!--><html lang="en"> <!--<![endif]-->
  <head>
    <meta charset="utf-8" />
     <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    {+metaTags/}
    <title>{+pageTitle}Links.to: Page{/pageTitle}</title>
    <link href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.1/css/bootstrap-combined.no-icons.min.css" rel="stylesheet"/>
    <link href="//netdna.bootstrapcdn.com/font-awesome/3.1.1/css/font-awesome.css" rel="stylesheet"/>
    <link href="/css/links-to.css" rel="stylesheet" />
    {+headLinks/}
    {+headContent/}
    <script>pageParam = { dummy:0 {+pageParam}{/pageParam} }</script>
    <script>
      function fallback(fn, src ){
        if ( !fn ) {  document.write(unescape("%3Cscript src='" + src + "' type='text/javascript'%3E%3C/script%3E"));  }
      }
    </script>
  </head>
  <body class="{+bodyClass/}">
    <div id="navbar">{>"navigation/navbar"/}</div>
    <div class="container" id="container">
      <div id="breadcrumbs">{>"navigation/breadcrumbs"/}</div>
      <div class="row"><div class="span8" id="title">{+title}<h1>{title}</h1>{/title}</div></div>
      <div class="row"><div class="span8 error">{error}</div></div>
      <div id="beforeContent">{+beforeContent/}</div>
      <div id="content">{+content}_content_{/content}</div>
      {+contentBottom/}
    </div>

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script type="text/javascript">fallback( window.jQuery, '/js/jquery-1.8.3.min.js'  );</script>
    <script type="text/javascript">
        // TODO find some other way to check for CDN CSS fallback
        if ( 0 &&  typeof jQuery == 'undefined') {
            document.write(unescape('%3Clink rel="stylesheet" type="text/css" href="/bootstrapcdn-offline/2.3.1/bootstrap-combined.no-icons.min.css" /%3E'));
            document.write(unescape('%3Clink rel="stylesheet" type="text/css" href="/font-awesome/css/font-awesome.min.css" /%3E'));
            document.write(unescape('%3Clink rel="stylesheet" type="text/css" href="/css/links-to.css" /%3E'));
        }
    </script>
    <script type="text/javascript" src="/js/page.js"></script>
    <script type="text/javascript" src="/socket.io/socket.io.js"></script>
    <script type="text/javascript" src="/js/links-to.js"></script>

    <script src="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.1/js/bootstrap.min.js"></script>
    <script type="text/javascript">fallback( $ && $.fn ? $.fn.affix : null, '/bootstrapcdn-offline/2.3.1/bootstrap.min.js'  );</script>
    {+pageBottom /}
  </body>
</html>