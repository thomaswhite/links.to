{#display.notFound}
  {#user}
    {@eq value="{user._id}" key="{owner}"}
      <div class="blocked-link" id="link_{_id}">
          {>"links/link_content" link_id="{_id}"/}
      </div>
    {/eq}
  {/user}
{:else}
  <div class="blocked-link" id="link_{_id}">
      {?display}
          {>"links/link_content" link_id="{_id}"/}
      {/display}
      {^display}
         <div>No link data.</div>
      {/display}
  </div>
{/display.notFound}
